{
  "name": "PC Queue",
  "description": "Displays a queue for the PC website.",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_icon": "favicon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://www.pokemoncenter.com/*"
      ],
      "all_frames": true
    }
  ]
}